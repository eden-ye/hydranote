meta {
  name: Get Block
  type: http
  seq: 3
}

get {
  url: {{baseUrl}}/api/blocks/{{createdBlockId}}
  body: none
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

assert {
  res.status: eq 200
  res.body.id: eq {{createdBlockId}}
  res.body.content.text: isString
  res.body.block_type: isString
}
